#include<iostream>
using namespace std;
int main(){
    int r1, c1, r2, c2;
    cout << "Enter rows and columns for first matrix: ";
    cin >> r1 >> c1;
    cout << "Enter rows and columns for second matrix: ";
    cin >> r2 >> c2;
    int a[r1][c1], b[r2][c2], c[r1][c2];
    for(int i=0;i<r1;i++){
        for(int j=0;j<c1;j++){
            cout << "Enter element a[" << i << "][" << j << "]: ";
            cin >> a[i][j]; 
        }
    }
    for(int i=0;i<r2;i++){
        for(int j=0;j<c2;j++){
            cout << "Enter element b[" << i << "][" << j << "]: ";
            cin >> b[i][j]; 
        }
    }
    if(c1 != r2) {
        cout << "Matrix multiplication not possible!" << endl;
        return 0;
    }
    for(int i=0;i<r1;i++){{
        for(int j=0;j<c2;j++){
            c[i][j]=0;
            for(int k=0;k<c1;k++){
                c[i][j]+=a[i][k]*b[k][j];
            }
        }
    }}
    for(int i=0;i<r1;i++){ 
        for(int j=0;j<c2;j++){
            cout << c[i][j] << " ";
        }
        cout << endl;
    }
    return 0;
    
}
