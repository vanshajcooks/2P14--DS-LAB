#include <iostream>
#include <algorithm>
#include <set>
#include <string>
#include <cctype>
using namespace std;

int binarySearch(int arr[], int n, int x) {
    int l=0,r=n-1;
    while(l<=r) {
        int m=(l+r)/2;
        if(arr[m]==x) return m;
        if(arr[m]<x) l=m+1;
        else r=m-1;
    }
    return -1;
}

void bubbleSort() {
    int arr[]={64,34,25,12,22,11,90},n=7;
    for(int i=0;i<n-1;i++)
        for(int j=0;j<n-i-1;j++)
            if(arr[j]>arr[j+1]) swap(arr[j],arr[j+1]);
    for(int i=0;i<n;i++) cout<<arr[i]<<" ";
    cout<<endl;
}

void missingLinear() {
    int arr[]={1,2,3,5,6},n=6;
    int sum=n*(n+1)/2,arrSum=0;
    for(int i=0;i<n-1;i++) arrSum+=arr[i];
    cout<<sum-arrSum<<endl;
}

int missingBinary(int arr[],int n){
    int l=0,r=n-1;
    while(l<=r){
        int m=(l+r)/2;
        if(arr[m]==m+1) l=m+1;
        else r=m-1;
    }
    return l+1;
}

void stringOps() {
    string s1="Hello",s2="World";
    cout<<s1+s2<<endl;
    string s="Hello";
    reverse(s.begin(),s.end());
    cout<<s<<endl;
    string t="HelloWorld",res="";
    for(char c:t) if(string("aeiouAEIOU").find(c)==string::npos) res+=c;
    cout<<res<<endl;
    string arr[]={"banana","apple","cherry"}; int n=3;
    sort(arr,arr+n);
    for(int i=0;i<n;i++) cout<<arr[i]<<" "; cout<<endl;
    char c='A';
    cout<<(char)tolower(c)<<endl;
}

void diagonalMatrix() {
    int n=4,diag[]={1,2,3,4};
    for(int i=0;i<n;i++){for(int j=0;j<n;j++){if(i==j) cout<<diag[i]<<" "; else cout<<0<<" ";}cout<<endl;}
}

void sparseMatrixTranspose() {
    struct E{int r,c,v;};
    E a[4]={{0,0,1},{0,2,2},{1,1,3},{2,0,4}};
    int n=4;
    for(int i=0;i<n;i++) cout<<a[i].c<<" "<<a[i].r<<" "<<a[i].v<<"\n";
}

void countInversions() {
    int arr[]={2,4,1,3,5},n=5,cnt=0;
    for(int i=0;i<n;i++) for(int j=i+1;j<n;j++) if(arr[i]>arr[j]) cnt++;
    cout<<cnt<<endl;
}

void countDistinct() {
    int arr[]={1,2,2,3,4,4},n=6;
    set<int>s(arr,arr+n);
    cout<<s.size()<<endl;
}

int main(){
    int choice;
    cout<<"1.Binary Search\n2.Bubble Sort\n3.Missing Number (Linear)\n4.Missing Number (Binary)\n5.String Operations\n6.Diagonal Matrix\n7.Sparse Matrix Transpose\n8.Count Inversions\n9.Count Distinct\n";
    cin>>choice;
    if(choice==1){int arr[]={10,20,30,40,50};cout<<binarySearch(arr,5,30)<<endl;}
    else if(choice==2) bubbleSort();
    else if(choice==3) missingLinear();
    else if(choice==4){int arr[]={1,2,3,5,6};cout<<missingBinary(arr,5)<<endl;}
    else if(choice==5) stringOps();
    else if(choice==6) diagonalMatrix();
    else if(choice==7) sparseMatrixTranspose();
    else if(choice==8) countInversions();
    else if(choice==9) countDistinct();
}
